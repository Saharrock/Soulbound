<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Soulbound.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Soulbound.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="Soulbound">


    <FlyoutItem>
        <ShellContent
    Title="Welcome"
    ContentTemplate="{DataTemplate local:WelcomePage}"
    Route="WelcomePage" />

        <ShellContent
    Title="LogIn"
    ContentTemplate="{DataTemplate local:LogInPage}"
    Route="LogInPage" />

        <ShellContent
    Title="Registration"
    ContentTemplate="{DataTemplate local:RegistrationPage}"
    Route="RegistrationPage" />

        <ShellContent
Title="MainRoom"
ContentTemplate="{DataTemplate local:MainRoomPage}"
Route="MainRoomPage" />
    </FlyoutItem>


</Shell>
